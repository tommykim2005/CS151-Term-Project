<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx/17"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="cs151.application.StudentListController"
            prefWidth="800.0" prefHeight="520.0">

    <center>
        <TableView fx:id="table">
            <columns>
                <TableColumn fx:id="full_Name" text="Full Name" prefWidth="220.0"/>
                <TableColumn fx:id="academic_Status" text="Academic Status" prefWidth="160.0"/>
                <TableColumn fx:id="current_Job_Status" text="Job Status" prefWidth="140.0"/>
                <TableColumn fx:id="job_Details" text="Job Details" prefWidth="260.0"/>
            </columns>
        </TableView>
    </center>
</BorderPane>
